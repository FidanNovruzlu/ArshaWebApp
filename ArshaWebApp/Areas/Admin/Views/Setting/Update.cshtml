
@model UpdateSettingVM

 <div class="row bg-title">
     <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
            <h4 class="page-title">Setting Update</h4> 
    </div>
 </div>
<div id="page-wrapper">
    <div class="row">
         <div class="col-md-8 col-xs-12">
              <div class="white-box">
                   <form class="form-horizontal form-material" method="post" enctype="multipart/form-data">
                       <div class="text-danger" asp-validation-summary="All"></div>
                        <div class="form-group">
                            <label class="col-md-12" asp-for="Value"></label>
                            <div class="col-md-12">
                                <input type="text" placeholder="Value" class="form-control form-control-line" asp-for="Value">
                                <span class="text-danger" asp-validation-for="Value"></span>
                            </div>
                        </div>
                      
                        <div class="form-group">
                            <div class="col-sm-12">
                                <button class="btn btn-success" type="submit" >Update</button>
                            </div>
                        </div>
                   </form>
              </div>
         </div>
    </div>
</div>